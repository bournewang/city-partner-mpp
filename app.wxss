/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

page{
  --td-brand-color: #8E44AD;
  --td-success-color: #27AE60;
  --td-warning-color: #F39C12;
  --td-error-color: #E74C3C;
}

.success-btn{
  background-color: var(--td-success-color);
  color: white;
}